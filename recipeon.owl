###  http://qudt.org/2.1/schema/qudt/Quantity
qudt:Quantity rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty qudt:hasQuantityValue ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass qudt:QuantityValue
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty qudt:hasUnit ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass qudt:Unit
                              ] ;
              rdf:about "https://en.wikipedia.org/wiki/Quantity"@en ;
              rdf:type owl:Class ;
              rdfs:comment "Quantity or amount is a property that can exist as a multitude or magnitude, which illustrate discontinuity and continuity. Quantities can be compared in terms of \"more\", \"less\", or \"equal\", or by assigning a numerical value multiple of a unit of measurement. Mass, time, distance, heat, and angle are among the familiar examples of quantitative properties."@en ;
              rdfs:isDefinedBy "https://qudt.org/2.1/schema/qudt"@en ;
              rdfs:label "Quantity"^^xsd:string .


###  http://qudt.org/2.1/schema/qudt/QuantityValue
qudt:QuantityValue rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty qudt:hasNumericValue ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onDataRange xsd:double
                                   ] ;
                   rdf:type owl:Class ;
                   rdfs:comment "Single non-negative integer value"@en ;
                   rdfs:isDefinedBy "https://qudt.org/2.1/schema/qudt"@en ;
                   rdfs:label "QuantatiyValue"^^xsd:string .


###  http://qudt.org/2.1/schema/qudt/Unit
qudt:Unit rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty qudt:Abbreviation ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] ;
          rdf:about "https://en.wikipedia.org/wiki/Unit_of_measurement"@en ;
          rdf:type owl:Class ;
          rdfs:comment "A unit of measurement is a definite magnitude of a quantity, defined and adopted by convention or by law, that is used as a standard for measurement of the same kind of quantity. Any other quantity of that kind can be expressed as a multiple of the unit of measurement."@en ;
          rdfs:isDefinedBy "https://qudt.org/2.1/schema/qudt"@en ;
          rdfs:label "Unit"^^xsd:string .


