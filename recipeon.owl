<https://purl.org/net/recipeon> a owl:Ontology ;
                                        owl:versionIRI <https://purl.org/net/recipeon/v/1.0> ;
                                        dc:publisher "Recipe Evolution Group" ;
                                        dc:title "RecipeOn Ontology" ;
                                        terms:contributor <https://hajirajabeen.github.io> ,
                                                          <https://github.com/fahad-maqbool> ,
                                                          <https://github.com/msaadlak> ;
                                        terms:created "2022/06/15"^^xsd:date ;
                                        terms:creator <https://github.com/msaadlak> ;
                                        terms:description "RecipeOn assists machines in understanding the semantics of recipes to encode the information crucial for personalized recipe generation. This includes nutritional information, ingredient-action relations, ingredient-diet relations, ingredient substitutions, and cooking time." ;
                                        terms:hasVersion 1.0 ;
                                        terms:license <https://creativecommons.org/licenses/by/4.0/> ;
                                        terms:modified "2023/03/22" ;
                                        terms:title "The RecipeOn Ontology (recipeon)"@en ;
                                        rdfs:comment "RecipeOn assists machines in understanding the semantics of recipes to encode the information crucial for personalized recipe generation. This includes nutritional information, ingredient-action relations, ingredient-diet relations, ingredient substitutions, and cooking time." ;
                                        rdfs:isDefinedBy <https://purl.org/net/recipeon> ;
                                        rdfs:label "The RecipeOn Ontology"@en ;
                                        rdfs:seeAlso <https://hajirajabeen.github.io/EvoRecipesOntology/> .

