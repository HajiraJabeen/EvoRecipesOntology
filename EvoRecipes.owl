###  http://www.w3.org/2002/07/owl#Action
owl:Action rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Class ;
                             owl:unionOf ( [ rdf:type owl:Restriction ;
                                             owl:onProperty seq:directlyFollows ;
                                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onClass owl:Action
                                           ]
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty evo:hasActionIngredient ;
