#################################################################
#    Classes
#################################################################

###  http://qudt.org/2.1/schema/qudt#Quantity
qudt:Quantity rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty qudt:hasQuantityValue ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass qudt:QuantityValue
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty qudt:hasUnit ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass qudt:Unit
                              ] .


###  http://qudt.org/2.1/schema/qudt#QuantityValue
qudt:QuantityValue rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty qudt:hasNumericValue ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onDataRange xsd:double
                                   ] .


###  http://qudt.org/2.1/schema/qudt#Unit
qudt:Unit rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty qudt:Abbreviation ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] .


###  http://schema.org/AggregateRating
<http://schema.org/AggregateRating> rdf:type owl:Class ;
                                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                      owl:onProperty <http://schema.org/ratingCount> ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onDataRange xsd:integer
                                                    ] ,
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty <http://schema.org/reviewCount> ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onDataRange xsd:integer
                                                    ] .


###  http://schema.org/NutritionInformation
<http://schema.org/NutritionInformation> rdf:type owl:Class ;
                                         rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://schema.org/calories> ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass qudt:Quantity
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://schema.org/carbohydrateContent> ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass qudt:Quantity
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://schema.org/cholesterolContent> ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass qudt:Quantity
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://schema.org/fatContent> ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass qudt:Quantity
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://schema.org/fiberContent> ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass qudt:Quantity
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://schema.org/proteinContent> ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass qudt:Quantity
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://schema.org/saturatedFatContent> ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass qudt:Quantity
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://schema.org/sodiumContent> ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass qudt:Quantity
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://schema.org/sugarContent> ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass qudt:Quantity
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://schema.org/transFatContent> ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass qudt:Quantity
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://schema.org/unsaturatedFatContent> ;
                                                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass qudt:Quantity
                                                         ] .


